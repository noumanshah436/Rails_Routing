<p id="notice"><%= notice %></p>

<h1>Admin Students</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_students.each do |admin_student| %>
      <tr>
        <td><%= admin_student.name %></td>
        <td><%= admin_student.email %></td>
        <td><%= link_to 'Show', admin_student %></td>
        <td><%= link_to 'Edit', edit_admin_student_path(admin_student) %></td>
        <td><%= link_to 'Destroy', admin_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Student', new_admin_student_path %>
